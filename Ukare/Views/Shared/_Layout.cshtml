                                        <li><a href="#">JOBSEEKERS</a></li>
                                        <li><a href="#">ASSESSMENT</a></li>
                                        <li><a href="#">CONTACT</a></li>


                                    </ul>
                                </div><!-- /.navbar-collapse -->
                            </div><!-- /.container-fluid -->
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </header-bottom>
    <!------header-end------>
    @RenderBody()

