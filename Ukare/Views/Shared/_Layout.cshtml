                            <li><i class="fa fa-facebook-square" aria-hidden="true"></i></li>
                            <li><i class="fa fa-twitter-square" aria-hidden="true"></i></li>
                            <li><i class="fa fa-google-plus-square" aria-hidden="true"></i></li>
                            <li><i class="fa fa-linkedin-square" aria-hidden="true"></i></li>
                        </ul>
                    </div>
                </div>
                <div class="col-md-3"></div>
                <div class="col-md-7">
                    <div class="head-contact">
                        <ul class="conat-list">
                            @if (HttpContext.Current.Session["UserId"] != null)
                            {
                                if (HttpContext.Current.Session["RoleId"].ToString()  == "1")
                                {
                            <li class="h-link"><a href="@Url.Action("Index", "Dashboard", new { area ="AdminArea"},null)">Admin Panel</a></li>}
                                <li>Email : @HttpContext.Current.Session["UserEmailId"].ToString()</li>
                            }
                            <li>Tel / fax : +441604 - 808988</li>
                            @if (HttpContext.Current.Session["UserId"] == null)
                            {
                                <li class="h-link">@Html.ActionLink("Login", "Login", "Home")</li>
                                <li class="h-link">@Html.ActionLink("Register", "Registration", "Home")</li>
                            }
                            else
                            {
                                <li class="h-link">@Html.ActionLink("Log Out", "LogOut", "Home")</li>
                            }

                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </header>

    <header-bottom>
